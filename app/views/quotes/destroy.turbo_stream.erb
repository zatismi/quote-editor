<%= turbo_stream.remove @quote %>
<%= render_turbo_stream_flash_messages %>
<%= turbo_stream.update "single_quote" do %>
  <% render "empty_placeholder" %>
<% end %>
<% unless current_company.quotes.exists? %>
  <%= turbo_stream.update "single_quote" do %>
    <%= render "quotes/empty_state" %>
  <% end %>
  <%= turbo_stream.update "quotes" do %>
    <%= render "quotes/empty_quotes_list_msg" %>
  <% end %>
<% end %>
